#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Matteo/Desktop/Pulsix/node_modules/.pnpm/vite@6.2.1_@types+node@22.1_4b09d6b81a3257ad2749b2b08dac57bc/node_modules/vite/bin/node_modules:/mnt/c/Users/Matteo/Desktop/Pulsix/node_modules/.pnpm/vite@6.2.1_@types+node@22.1_4b09d6b81a3257ad2749b2b08dac57bc/node_modules/vite/node_modules:/mnt/c/Users/Matteo/Desktop/Pulsix/node_modules/.pnpm/vite@6.2.1_@types+node@22.1_4b09d6b81a3257ad2749b2b08dac57bc/node_modules:/mnt/c/Users/Matteo/Desktop/Pulsix/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Matteo/Desktop/Pulsix/node_modules/.pnpm/vite@6.2.1_@types+node@22.1_4b09d6b81a3257ad2749b2b08dac57bc/node_modules/vite/bin/node_modules:/mnt/c/Users/Matteo/Desktop/Pulsix/node_modules/.pnpm/vite@6.2.1_@types+node@22.1_4b09d6b81a3257ad2749b2b08dac57bc/node_modules/vite/node_modules:/mnt/c/Users/Matteo/Desktop/Pulsix/node_modules/.pnpm/vite@6.2.1_@types+node@22.1_4b09d6b81a3257ad2749b2b08dac57bc/node_modules:/mnt/c/Users/Matteo/Desktop/Pulsix/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@6.2.1_@types+node@22.1_4b09d6b81a3257ad2749b2b08dac57bc/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@6.2.1_@types+node@22.1_4b09d6b81a3257ad2749b2b08dac57bc/node_modules/vite/bin/vite.js" "$@"
fi

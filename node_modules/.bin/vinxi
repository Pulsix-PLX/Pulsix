#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Pallassini/Desktop/Pulsix/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._f662fdc5a508e4a4c66410795cb83d1d/node_modules/vinxi/bin/node_modules:/mnt/c/Users/Pallassini/Desktop/Pulsix/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._f662fdc5a508e4a4c66410795cb83d1d/node_modules/vinxi/node_modules:/mnt/c/Users/Pallassini/Desktop/Pulsix/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._f662fdc5a508e4a4c66410795cb83d1d/node_modules:/mnt/c/Users/Pallassini/Desktop/Pulsix/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Pallassini/Desktop/Pulsix/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._f662fdc5a508e4a4c66410795cb83d1d/node_modules/vinxi/bin/node_modules:/mnt/c/Users/Pallassini/Desktop/Pulsix/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._f662fdc5a508e4a4c66410795cb83d1d/node_modules/vinxi/node_modules:/mnt/c/Users/Pallassini/Desktop/Pulsix/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._f662fdc5a508e4a4c66410795cb83d1d/node_modules:/mnt/c/Users/Pallassini/Desktop/Pulsix/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vinxi@0.5.3_@types+node@22._f662fdc5a508e4a4c66410795cb83d1d/node_modules/vinxi/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/vinxi@0.5.3_@types+node@22._f662fdc5a508e4a4c66410795cb83d1d/node_modules/vinxi/bin/cli.mjs" "$@"
fi
